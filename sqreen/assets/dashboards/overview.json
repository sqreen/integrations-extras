                    {
                        "id": 2608445837314408,
                        "definition": {
                            "title": "# Requests blocked",
                            "title_size": "16",
                            "title_align": "left",
                            "type": "query_value",
                            "requests": [
                                {
                                    "conditional_formats": [
                                        {
                                            "comparator": ">=",
                                            "palette": "white_on_red",
                                            "value": 1000
                                        },
                                        {
                                            "comparator": ">=",
                                            "palette": "white_on_yellow",
                                            "value": 100
                                        },
                                        {
                                            "comparator": ">=",
                                            "palette": "black_on_light_yellow",
                                            "value": 10
                                        },
                                        {
                                            "comparator": "<",
                                            "palette": "yellow_on_white",
                                            "value": 10
                                        }
                                    ],
                                    "log_query": {
                                        "index": "*",
                                        "search": {
                                            "query": "$Service $Env @sqreen.context.http.response.blocked:true $Service $Env $User $IPAddress $Country"
                                        },
                                        "group_by": [],
                                        "compute": {
                                            "aggregation": "count"
                                        }
                                    }
                                }
                            ],
                            "autoscale": true,
                            "custom_unit": "reqs",
                            "precision": 2
                        },
                        "layout": {
                            "x": 0,
                            "y": 1,
                            "width": 2,
                            "height": 2
                        }
                    },
                    {
                        "id": 6244161677617408,
                        "definition": {
